{
    "author": "Ty Eggen",
    "dependencies": {
        "@eslint/js": "^9.32.0",
        "deepmerge": "^4.3.1",
        "eslint-config-love": "^121.0.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^16.3.0",
        "rust-just": "^1.42.4"
    },
    "description": "various eslint configs",
    "devDependencies": {
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint-plugin-jsx-a11y": "^6.10.0",
        "@types/node": "^20.17.1",
        "@types/react": "^18.3.12",
        "@typescript-eslint/utils": "^8.39.0",
        "eslint": "^9.32.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "just-install": "^2.0.2",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "tsup": "^8.5.0",
        "typescript": "^5.6.3",
        "wireit": "^0.14.12"
    },
    "engines": {
        "node": ">=20"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.mts"
        },
        "./cjs": {
            "import": "./dist/cjs.mjs",
            "types": "./dist/cjs.d.mts"
        },
        "./cjsx": {
            "import": "./dist/cjsx.mjs",
            "types": "./dist/cjsx.d.mts"
        },
        "./combined": {
            "import": "./dist/combined.mjs",
            "types": "./dist/combined.d.mts"
        },
        "./cts": {
            "import": "./dist/cts.mjs",
            "types": "./dist/cts.d.mts"
        },
        "./ctsx": {
            "import": "./dist/ctsx.mjs",
            "types": "./dist/ctsx.d.mts"
        },
        "./js": {
            "import": "./dist/js.mjs",
            "types": "./dist/js.d.mts"
        },
        "./jsx": {
            "import": "./dist/jsx.mjs",
            "types": "./dist/jsx.d.mts"
        },
        "./mjs": {
            "import": "./dist/mjs.mjs",
            "types": "./dist/mjs.d.mts"
        },
        "./mjsx": {
            "import": "./dist/mjsx.mjs",
            "types": "./dist/mjsx.d.mts"
        },
        "./mts": {
            "import": "./dist/mts.mjs",
            "types": "./dist/mts.d.mts"
        },
        "./mtsx": {
            "import": "./dist/mtsx.mjs",
            "types": "./dist/mtsx.d.mts"
        },
        "./ts": {
            "import": "./dist/ts.mjs",
            "types": "./dist/ts.d.mts"
        },
        "./tsx": {
            "import": "./dist/tsx.mjs",
            "types": "./dist/tsx.d.mts"
        }
    },
    "files": [
        "./dist"
    ],
    "keywords": [],
    "license": "ISC",
    "main": "./dist/index.mjs",
    "name": "@tyisi/config-eslint",
    "peerDependencies": {
        "eslint": "^9.12.0",
        "prettier": "^3.3.3"
    },
    "repository": "https://github.com/TyIsI/configs.git",
    "scripts": {
        "build": "wireit",
        "build:check": "wireit",
        "build:src": "wireit",
        "commit": "npx git-cz@latest --scope=eslint",
        "lint": "wireit",
        "type-check": "wireit"
    },
    "type": "module",
    "types": "./dist/index.d.mts",
    "version": "4.0.0",
    "wireit": {
        "build": {
            "dependencies": [
                "type-check",
                "build:src",
                "build:check"
            ]
        },
        "build:check": {
            "command": "just build-check",
            "dependencies": [
                "build:src"
            ]
        },
        "build:src": {
            "command": "just build",
            "files":["src/"],
            "output":["dist/**"],
            "dependencies": [
                "type-check"
            ]
        },
        "lint": {
            "command": "just lint",
            "dependencies": [
                "type-check"
            ]
        },
        "type-check": {
            "command": "just type-check"
        }
    }
}
