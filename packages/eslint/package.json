{
    "name": "@tyisi/config-eslint",
    "author": "Ty Eggen",
    "description": "various eslint configs",
    "devDependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "4.6.0",
        "@eslint/js": "9.39.2",
        "@tyisi/config-prettier": "workspace:*",
        "@types/eslint-plugin-jsx-a11y": "6.10.1",
        "@types/node": "^24.10.9",
        "@types/react": "^19.2.10",
        "@typescript-eslint/utils": "8.54.0",
        "deepmerge": "4.3.1",
        "eslint": "9.39.2",
        "eslint-config-love": "149.0.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-n": "^17.23.2",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "7.37.5",
        "git-cz": "4.9.0",
        "globals": "17.2.0",
        "just-install": "2.0.2",
        "prettier": "3.8.1",
        "react": "^19.2.4",
        "tsup": "8.5.1",
        "tsx": "^4.21.0",
        "typescript": "5.9.3",
        "typescript-eslint": "^8.54.0",
        "wireit": "0.14.12"
    },
    "engines": {
        "node": ">=20"
    },
    "exports": {
        ".": {
            "types": {
                "require": "./dist/all.d.cts",
                "import": "./dist/all.d.mts"
            },
            "require": "./dist/all.cjs",
            "import": "./dist/all.mjs"
        },
        "./all": {
            "types": {
                "require": "./dist/all.d.cts",
                "import": "./dist/all.d.mts"
            },
            "require": "./dist/all.cjs",
            "import": "./dist/all.mjs"
        },
        "./cjs": {
            "types": {
                "require": "./dist/cjs.d.cts",
                "import": "./dist/cjs.d.mts"
            },
            "require": "./dist/cjs.cjs",
            "import": "./dist/cjs.mjs"
        },
        "./cjsx": {
            "types": {
                "require": "./dist/cjsx.d.cts",
                "import": "./dist/cjsx.d.mts"
            },
            "require": "./dist/cjsx.cjs",
            "import": "./dist/cjsx.mjs"
        },
        "./combined": {
            "types": {
                "require": "./dist/combined.d.cts",
                "import": "./dist/combined.d.mts"
            },
            "require": "./dist/combined.cjs",
            "import": "./dist/combined.mjs"
        },
        "./cts": {
            "types": {
                "require": "./dist/cts.d.cts",
                "import": "./dist/cts.d.mts"
            },
            "require": "./dist/cts.cjs",
            "import": "./dist/cts.mjs"
        },
        "./ctsx": {
            "types": {
                "require": "./dist/ctsx.d.cts",
                "import": "./dist/ctsx.d.mts"
            },
            "require": "./dist/ctsx.cjs",
            "import": "./dist/ctsx.mjs"
        },
        "./js": {
            "types": {
                "require": "./dist/js.d.cts",
                "import": "./dist/js.d.mts"
            },
            "require": "./dist/js.cjs",
            "import": "./dist/js.mjs"
        },
        "./jsx": {
            "types": {
                "require": "./dist/jsx.d.cts",
                "import": "./dist/jsx.d.mts"
            },
            "require": "./dist/jsx.cjs",
            "import": "./dist/jsx.mjs"
        },
        "./mjs": {
            "types": {
                "require": "./dist/mjs.d.cts",
                "import": "./dist/mjs.d.mts"
            },
            "require": "./dist/mjs.cjs",
            "import": "./dist/mjs.mjs"
        },
        "./mjsx": {
            "types": {
                "require": "./dist/mjsx.d.cts",
                "import": "./dist/mjsx.d.mts"
            },
            "require": "./dist/mjsx.cjs",
            "import": "./dist/mjsx.mjs"
        },
        "./mts": {
            "types": {
                "require": "./dist/mts.d.cts",
                "import": "./dist/mts.d.mts"
            },
            "require": "./dist/mts.cjs",
            "import": "./dist/mts.mjs"
        },
        "./mtsx": {
            "types": {
                "require": "./dist/mtsx.d.cts",
                "import": "./dist/mtsx.d.mts"
            },
            "require": "./dist/mtsx.cjs",
            "import": "./dist/mtsx.mjs"
        },
        "./ts": {
            "types": {
                "require": "./dist/ts.d.cts",
                "import": "./dist/ts.d.mts"
            },
            "require": "./dist/ts.cjs",
            "import": "./dist/ts.mjs"
        },
        "./tsx": {
            "types": {
                "require": "./dist/tsx.d.cts",
                "import": "./dist/tsx.d.mts"
            },
            "require": "./dist/tsx.cjs",
            "import": "./dist/tsx.mjs"
        }
    },
    "files": [
        "./dist",
        "./package.json",
        "./README.md"
    ],
    "keywords": [],
    "license": "ISC",
    "main": "./dist/all.mjs",
    "peerDependencies": {
        "eslint": "9.39.2",
        "prettier": "3.8.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": "TyIsI/configs",
    "scripts": {
        "build": "wireit",
        "build:check": "wireit",
        "build:src": "wireit",
        "commit": "git-cz --scope=eslint",
        "lint": "wireit",
        "type-check": "wireit"
    },
    "type": "module",
    "types": "./dist/all.d.mts",
    "version": "6.0.0",
    "wireit": {
        "build": {
            "dependencies": [
                "type-check",
                "build:src",
                "build:check"
            ]
        },
        "build:check": {
            "command": "just build_check",
            "dependencies": [
                "build:src"
            ],
            "files": [
                "src/"
            ]
        },
        "build:src": {
            "command": "just build",
            "dependencies": [
                "type-check"
            ],
            "files": [
                "lib/**/*",
                "src/**/*",
                "tools/fix-dist-extensions.sh",
                "tsup.config.ts"
            ],
            "output": [
                "dist/**"
            ]
        },
        "lint": {
            "command": "just lint",
            "dependencies": [
                "type-check"
            ]
        },
        "type-check": {
            "command": "just type-check",
            "files": [
                "src/**/*.ts"
            ]
        }
    }
}
