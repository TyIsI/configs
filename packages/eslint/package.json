{
    "author": "Ty Eggen",
    "dependencies": {
        "@eslint/js": "^9.12.0",
        "eslint-config-love": "^87.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-react": "^7.37.1",
        "globals": "^15.11.0"
    },
    "description": "various eslint configs",
    "devDependencies": {
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint-plugin-jsx-a11y": "^6.9.0",
        "@types/eslint__js": "^8.42.3",
        "@typescript-eslint/utils": "^8.11.0",
        "eslint": "^9.12.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "prettier": "^3.3.3",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3",
        "wireit": "^0.14.9"
    },
    "main": "./dist/config.cjs",
    "types": "./dist/config.d.cts",
    "exports": {
        "require": "./dist/config.cjs",
        "types": "./dist/config.d.mts",
        "import": "./dist/config.mjs"
    },
    "files": [
        "./dist"
    ],
    "keywords": [],
    "license": "ISC",
    "name": "@tyisi/config-eslint",
    "peerDependencies": {
        "eslint": "^9.12.0",
        "prettier": "^3.3.3"
    },
    "repository": "https://github.com/TyIsI/configs.git",
    "scripts": {
        "build": "wireit",
        "build:check": "wireit",
        "build:cjs": "wireit",
        "build:esm": "wireit",
        "commit": "npx git-cz@latest --scope=eslint",
        "lint": "wireit",
        "type-check": "wireit"
    },
    "version": "1.0.1",
    "wireit": {
        "build": {
            "dependencies": [
                "build:cjs",
                "build:esm",
                "type-check",
                "build:check"
            ]
        },
        "build:check": {
            "dependencies": [
                "build:cjs",
                "build:esm",
                "type-check"
            ],
            "command": "pnpm dlx @arethetypeswrong/cli@latest --pack ."
        },
        "build:cjs": {
            "clean": true,
            "command": "tsup --cjsInterop --format cjs --dts",
            "dependencies": [
                "type-check"
            ],
            "files": [
                "src/config.ts"
            ],
            "output": [
                "dist/config.cjs"
            ]
        },
        "build:esm": {
            "clean": true,
            "command": "tsup --format esm --dts",
            "dependencies": [
                "type-check"
            ],
            "files": [
                "src/config.ts"
            ],
            "output": [
                "dist/config.mjs"
            ]
        },
        "lint": {
            "dependencies": [
                "lint:code",
                "lint:format"
            ]
        },
        "lint:code": {
            "command": "eslint --fix src/ types/"
        },
        "lint:format": {
            "command": "prettier -w .",
            "dependencies": [
                "lint:code"
            ]
        },
        "type-check": {
            "command": "tsc --noEmit"
        }
    }
}
